{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "contact-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/contactList/ContactListWebPart.js",
          "manifest": "./src/webparts/contactList/ContactListWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
      "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "globalName": "bootstrap"
    }
  },
  "localizedResources": {
    "ContactListWebPartStrings": "lib/webparts/contactList/loc/{locale}.js"
  }
}
